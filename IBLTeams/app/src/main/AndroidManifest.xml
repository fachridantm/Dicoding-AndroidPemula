<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.iblteams">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_ibl"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreenActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutMe" android:parentActivityName="com.dicoding.iblteams.MainActivity" />
        <activity android:name=".ProfileTeams" android:parentActivityName="com.dicoding.iblteams.MainActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>